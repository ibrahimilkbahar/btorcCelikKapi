<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
      	http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">
	
	<view-state id="listOrder">
		<transition on="detailOrder" to="detailOrder">
			<evaluate expression="orderController.initializeDetailOrder()" />
		</transition>
	</view-state>
	

	
	<view-state id="detailOrder">
		<transition on="back" to="listOrder">
			<evaluate expression="orderController.initOrderList()" />
		</transition>
		<transition on="orderSaved" to="detailOrder"/>
			
	</view-state>
	
	<end-state id="finish"/>
</flow>