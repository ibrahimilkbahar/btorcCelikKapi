<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
      	http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

 
	 
	<view-state id="proposalApproveList">
		<transition on="proposalApprove" to="proposalDetail">
			<evaluate expression="proposalApproveController.initialize()" />
		</transition>
	</view-state>
	
	<view-state id="proposalDetail">
		<transition on="back" to="finish"/>
		<transition on="proposalOfferingDetail" to="proposalOfferingDetail">
			<evaluate expression="proposalApproveController.initialize()" />
		</transition>
	</view-state>
	
	<view-state id="proposalOfferingDetail">
		<transition on="back" to="proposalDetail">
			<evaluate expression="proposalApproveController.initialize()" />
		</transition>
	</view-state>
	
	<end-state id="finish"/>
</flow>